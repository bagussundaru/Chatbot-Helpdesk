{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "maxDuration": 10,
        "pythonPath": ".",
        "pipInstallArgs": "-r requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/logo/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}